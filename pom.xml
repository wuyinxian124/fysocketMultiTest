<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>edu.scut.wusir</groupId>
	<artifactId>socketfyMultiTest</artifactId>
	<version>1.1.0</version>
	<packaging>jar</packaging>

	<name>socketfyMultiTest</name>
	<url>http://maven.apache.org</url>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.2</version>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
					<encoding>UTF8</encoding>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencies>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.2</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>com.fy.socket</groupId>
			<artifactId>fysocketAPPClient</artifactId>
			<version>3.1.0</version>
			<!-- 为了测试，而开发的基于netty 的客户端 3.1.0-->
		</dependency>
		<!-- <dependency> <groupId>org.java-websocket</groupId> <artifactId>Java-WebSocket</artifactId> 
			<version>1.3.0</version> </dependency> -->

		<dependency>
			<groupId>com.fy.socket</groupId>
			<artifactId>fysocket</artifactId>
			<version>2.1.0</version>
			<!-- 为fy 新编译版本 -->
			 <!-- 使用netty作为基础，只在测试包中使用 2.1.0 -->
		</dependency>
	</dependencies>
</project>
